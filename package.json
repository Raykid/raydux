{
  "name": "raydux",
  "version": "0.1.0",
  "description": "基于Redux包装的使用Hooks开发的轻量级状态仓库，全TypeScript支持",
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raykid/raydux.git"
  },
  "keywords": [
    "redux",
    "hooks",
    "TypeScript",
    "state",
    "store"
  ],
  "author": "raykid13",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Raykid/raydux/issues"
  },
  "homepage": "https://github.com/Raykid/raydux#readme",
  "lint-staged": {
    "*": [
      "npm run format"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged concurrent false"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@gfx/zopfli": "^1.0.15",
    "@types/lodash-es": "^4.17.12",
    "@types/webpack-env": "^1.18.8",
    "clean-webpack-plugin": "^4.0.0",
    "commander": "^13.1.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "peerDependencies": {
    "@types/react": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "mutative": "^1.0.0",
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-redux": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "react-router-dom": "^6.0.0 || ^7.0.0",
    "redux": "^4.0.0 || ^5.0.0"
  }
}
